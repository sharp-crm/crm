{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "build:clean": "npm run clean && npm run build",
    "init-db": "ts-node src/utils/initDatabase.ts",
    "dev:init": "npm run init-db && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "dynamodb:start": "sam local start-dynamodb",
    "dynamodb:stop": "docker stop $(docker ps -q --filter ancestor=amazon/dynamodb-local)",
    "dev:local": "sam local start-dynamodb & npm run dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.839.0",
    "@aws-sdk/lib-dynamodb": "^3.839.0",
    "aws-jwt-verify": "^4.0.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.150",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.0.7",
    "@types/uuid": "^9.0.8",
    "concurrently": "^9.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
